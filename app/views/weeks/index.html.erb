<h1>Listing weeks</h1>

<table>
  <tr>
    <th>Number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @weeks.each do |week| %>
  <tr>
    <td><%= week.number %></td>
    <td><%= link_to 'Show', [current_season, week] %></td>
    <td><%= link_to 'Edit', edit_season_week_path(current_season, week) %></td>
    <td><%= link_to 'Destroy', [current_season, week], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Week', new_season_week_path(current_season) %>
